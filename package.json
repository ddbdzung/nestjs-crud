{
  "name": "nestjs-crud",
  "version": "0.0.1",
  "description": "NestJS CRUD API",
  "author": "Dang Duc B. Dzung (David)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist tsconfig*.tsbuildinfo && nest build",
    "dev": "cross-env NODE_ENV=development nest start --watch",
    "start": "cross-env NODE_ENV=development nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node -r tsconfig-paths/register dist/main.js",
    "lint": "eslint src --fix --cache",
    "format": "prettier --write src",
    "type-check": "tsc -p ./tsconfig.json --noEmit",
    "type-check:watch": "tsc -p ./tsconfig.json --noEmit -w",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:cov": "cross-env NODE_ENV=test jest --coverage",
    "test:debug": "cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json",
    "prepare": "husky || true",
    "commit": "git-cz",
    "clean": "rm -rf dist tsconfig*.tsbuildinfo"
  },
  "dependencies": {
    "@fastify/compress": "^8.3.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/static": "^8.3.0",
    "@nestjs/common": "^11.1.3",
    "@nestjs/core": "^11.1.3",
    "@nestjs/microservices": "^11.1.9",
    "@nestjs/mongoose": "^11.0.3",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/platform-fastify": "^11.1.9",
    "@nestjs/swagger": "^11.2.3",
    "@nestjs/terminus": "^11.0.0",
    "@types/lodash": "^4.17.21",
    "@types/morgan": "^1.9.10",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cross-env": "^10.1.0",
    "custom-env": "^2.0.6",
    "dayjs": "^1.11.19",
    "debug": "^4.4.3",
    "fastify": "^5.6.2",
    "ioredis": "^5.8.2",
    "ip": "^2.0.1",
    "jsonwebtoken": "^9.0.3",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "mongodb": "^7.0.0",
    "mongoose": "^8.20.2",
    "morgan": "^1.10.1",
    "ms": "^2.1.3",
    "nestjs-cls": "^6.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.29.0",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.3",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.12.6",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.3",
    "@types/ip": "^1.1.3",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ms": "^2.1.0",
    "@types/node": "^22.15.32",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.3",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.0",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  },
  "engines": {
    "node": ">=20",
    "pnpm": ">=10"
  },
  "_moduleAliases": {
    "@core": "dist/core/index",
    "@modules": "dist/modules",
    "@config": "dist/config/index"
  },
  "packageManager": "pnpm@10.12.4",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
